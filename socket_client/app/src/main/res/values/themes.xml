<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.SocketClient" parent="Base.Theme.MaterialComponents.Light.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/grey_500</item>
        <item name="colorPrimaryVariant">@color/grey_900</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/green_700</item>
        <item name="colorSecondaryVariant">@color/green_900</item>
        <item name="colorOnSecondary">@color/black</item>

        <item name="android:colorBackground" tools:targetApi="l">@color/white</item>
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
    </style>
</resources>
